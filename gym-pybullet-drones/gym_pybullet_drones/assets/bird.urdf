<?xml version="1.0" ?>
<robot name="bird_model">

  <link name="base_link"> 
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.5"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>

    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="./model.dae" scale="1 1 1"/>
      </geometry> 
      <material name="bird_color">
        <color rgba="1 1 1 1"/>
      </material> 
    </visual>

    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="./model.dae" scale="1 1 1"/>
      </geometry>
    </collision>  
  </link>

</robot>
